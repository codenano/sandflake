extends app_layout
block upnav
  #loadCont
    #load
  #meatlaunch.kudos
  nav(class='navbar navbar-default navbar-fixed-top' role='navigation')
    .container-fluid
      a(id='menuLogo' class='navbar-brand' ng-click='go("/")')
      ul(id='menuList' class='nav navbar-nav')
      ul(class='nav navbar-nav navbar-right')
        li#auth.dropdown
          a#authBar.navbar-link.dropdown-toggle(data-toggle="dropdown")
          ul#authInfo.dropdown-menu.dropdown-menu
block content
  div(ng-view)
block downnav
  nav(id='downnav' class='navbar navbar-default navbar-fixed-bottom' role='navigation')
    .container-fluid
      .navbar-header
        button(id='subnavCollapse' type='button' class='navbar-toggle' data-toggle='collapse' data-target='#subnav')
      div(class='collapse nav-collapse' id='subnav')
        ul(class='nav navbar-nav')
        ul(id='subnavCont' class='nav navbar-nav navbar-right')
          li#meatlist.dropdown
            a.navbar-link.dropdown-toggle(data-toggle='dropdown')
            ul#meatList.dropdown-menu